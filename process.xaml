<Window x:Class="Seva.process"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Seva"
        mc:Ignorable="d"
        Title="process" Height="150" Width="400" ResizeMode="NoResize" WindowStyle="None" Background="#2A2A2A">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="processtext" Text="" Foreground="White" FontSize="15" VerticalAlignment="Bottom" Margin="30,0,0,5"/>
        <ProgressBar x:Name="proccessbar" Grid.Row="1" VerticalAlignment="Top" Margin="30,0,30,0" Height="15" Background="#2A2A2A" BorderThickness="0"/>
    </Grid>
</Window>
